<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="tv" class="ex06.springcontainer.di.xmlns.p.LgTV" p:speaker-ref="apple" p:price="300000">
		<!-- <property name="speaker" ref="apple"/>
		<property name="price" value="5000000"/> 하단의 Namespaces의 P를 클릭하면 이부분을 불필요-->
	</bean>
	<bean id="sony" class="ex06.springcontainer.di.xmlns.p.SonySpeaker"/>	
	<bean id="apple" class="ex06.springcontainer.di.xmlns.p.AppleSpeaker"/>	

</beans>
